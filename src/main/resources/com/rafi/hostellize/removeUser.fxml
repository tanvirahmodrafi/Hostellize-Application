<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" styleClass="anchor-pane" stylesheets="@css/createAdmin.css" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.rafi.hostellize.controller.RemoveUserController">
   <children>
      <Label layoutX="281.0" layoutY="82.0" text="Welcome back" textFill="WHITE">
         <font>
            <Font name="System Bold" size="38.0" />
         </font>
      </Label>
      <TextField fx:id="userIdField" layoutX="287.0" layoutY="221.0" prefHeight="34.0" prefWidth="226.0" promptText="User ID" />
      <Label layoutX="292.0" layoutY="195.0" prefHeight="26.0" prefWidth="215.0" text="Enter the UserId we want to remove." textFill="WHITE">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <CheckBox fx:id="checkBox" layoutX="287.0" layoutY="291.0" mnemonicParsing="false" text="Are you sure you want to remove this person." textFill="WHITE">
         <font>
            <Font size="14.0" />
         </font>
      </CheckBox>
      <Button layoutX="331.0" layoutY="396.0" mnemonicParsing="false" onAction="#removeAction" prefHeight="34.0" prefWidth="139.0" text="Remove" />
      <Label fx:id="successLabel" layoutX="315.0" layoutY="337.0" prefHeight="19.0" prefWidth="171.0" textFill="WHITE">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Label>
      <Button layoutX="708.0" layoutY="27.0" mnemonicParsing="false" onAction="#cancelAction" text="Cancel" />
   </children>
</AnchorPane>
